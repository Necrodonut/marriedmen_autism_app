<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:defaultValue="1"
        android:entries="@array/pref_time_frame_values"
        android:entryValues="@array/pref_time_frame_values"
        android:key="data_frequency"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_data_length_days" />

    <ListPreference
        android:saveEnabled="true"
        android:title="Graph Styles"
        android:key="graph_styles"
        android:entries="@array/graph_styles"
        android:entryValues="@array/graph_styles"
        android:defaultValue="Line"
        android:summary="%s"

        />

</PreferenceScreen>